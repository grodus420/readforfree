function jumpWall() {
  let spans = document.getElementsById("view-offer");
  spans.style.display='none';
  
  for (let i = 0; i < spans.length; ++i) {
  if (spans.innerHTML === "offer" || spans.innerHTML.includes("This story is for Tribune subscribers")) {
    let card = spans.closest("offer");
    card.setAttribute("style", "display: none !important;");
  }
  }
}

jumpWall();


    
